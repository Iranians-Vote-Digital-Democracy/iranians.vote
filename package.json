    "loglevel": "^1.8.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-retro": "^0.0.1",
    "pinia": "^2.0.28",
    "postcss": "^8.4.31",
    "uuid": "^9.0.0",
    "vue": "3",
    "vue-i18n": "^9.2.2",
